<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="hybrid.rdf.storage.domain.dao.SpoDaoSRepo">
    <insert id="save">
        INSERT INTO spos
            (s, p, o)
        VALUE
            (#{s}, #{p}, #{o})
    </insert>
</mapper>